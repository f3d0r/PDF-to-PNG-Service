{
  "author": "Fedor Paretsyk",
  "bugs": {
    "url": "https://github.com/f3d0r/PDF-to-PNG-Service/issues"
  },
  "dependencies": {
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "multer": "^1.4.2",
    "pdf-image": "^2.0.0",
    "pdf-parse": "^1.1.1",
    "unique-string": "^2.0.0"
  },
  "description": "A PDF to PNG Service, converting PDFs to PNG images, retrievable through GET requests.",
  "devDependencies": {
    "ajv": "^6.10.2",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-sort-requires": "^2.1.0",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "plugin:node/recommended",
      "prettier"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 9
    },
    "plugins": [
      "prettier",
      "json",
      "sort-requires"
    ],
    "rules": {
      "import/order": 0,
      "node/exports-style": [
        "error",
        "module.exports"
      ],
      "node/prefer-global/buffer": [
        "error",
        "always"
      ],
      "node/prefer-global/console": [
        "error",
        "always"
      ],
      "node/prefer-global/process": [
        "error",
        "always"
      ],
      "node/prefer-global/url": [
        "error",
        "always"
      ],
      "node/prefer-global/url-search-params": [
        "error",
        "always"
      ],
      "prettier/prettier": 0,
      "sort-keys": [
        "error",
        "asc",
        {
          "caseSensitive": true,
          "natural": true
        }
      ],
      "sort-requires/sort-requires": 2
    }
  },
  "homepage": "https://github.com/f3d0r/PDF-to-PNG-Service#readme",
  "license": "MIT",
  "main": "index.js",
  "name": "pdf_to_png_service",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f3d0r/PDF-to-PNG-Service.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "version": "1.0.0"
}
